<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <gradient
                android:startColor="#ded9d9"
                android:endColor="#ded9d9"
                android:angle="270" />
            <corners
                android:radius="35dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#ded9d9"
                android:endColor="#ded9d9"
                android:angle="90" />
            <corners
                android:radius="35dp" />
        </shape>
    </item>
</selector>